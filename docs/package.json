{
  "name": "sfeirschoolnestjs",
  "version": "1.0.0",
  "description": "Sfeir School Nest js with revealJS",
  "scripts": {
    "sass": "sass --watch ./scss/:css/ --source-map",
    "sass-once": "sass /scss/:css/ --source-map",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "live-server --port=4242 --open=\"./index.html\" --watch=\"markdown,scripts,scss/\"",
    "start": "parallelshell \"npm run serve\" \"npm run sass\"",
    "copy-reveal": "cpx \"node_modules/reveal.js/**/*\" \"web_modules/reveal.js\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfeir-open-source/sfeir-school-template.git"
  },
  "keywords": [
    "xxx",
    "sfeir"
  ],
  "authors": [
    "Nicolas Frizzarin <frizzarin.n@sfeir.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfeir-open-source/sfeir-school-template/issues"
  },
  "homepage": "https://github.com/sfeir-open-source/sfeir-school-template",
  "dependencies": {
    "highlight.js": "^9.15.6",
    "lit-html": "^1.3.0",
    "reveal.js": "^3.9.2",
    "sass": "^1.44.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "live-server": "^1.2.1",
    "parallelshell": "3.0.1",
    "snowpack": "1.6.0"
  },
  "snowpack": {
    "webDependencies": [
      "sfeir-school-theme/dist/**/*",
      "lit-html",
      "highlight.js/styles/**/*"
    ]
  }
}
